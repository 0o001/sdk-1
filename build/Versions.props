    <SystemReflectionMetadataVersion>1.4.2</SystemReflectionMetadataVersion>
  </PropertyGroup>

  <!-- Test Dependencies -->
  <PropertyGroup>
    <DotNetXUnitVersion>2.3.0</DotNetXUnitVersion>
    <FluentAssertionsVersion>4.19.2</FluentAssertionsVersion>
    <FluentAssertionsJsonVersion>4.19.0</FluentAssertionsJsonVersion>
    <MicrosoftDotNetCliUtilsVersion>2.0.0-preview3-006805</MicrosoftDotNetCliUtilsVersion>
    <MicrosoftNETTestSdkVersion>15.0.0</MicrosoftNETTestSdkVersion>
    <XUnitVersion>2.3.0</XUnitVersion>
    <XUnitConsoleVersion>2.3.0</XUnitConsoleVersion>
    <XUnitConsoleRunnerVersion>2.3.0</XUnitConsoleRunnerVersion>
