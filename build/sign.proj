<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  
  <!-- 
	PipeBuild overrides:	
    PB_SignType            {''|'public'|'real'|'test'}    Disables signing if set to a value other than 'real' 
	-->

  <Target Name="Sign" DependsOnTargets="_Sign"/>

  <PropertyGroup>
    <_SignDependsOn>
      $(_SignDependsOn);
    </_SignDependsOn>
  </PropertyGroup>

  <Target Name="_Sign" DependsOnTargets="$(_SignDependsOn)" />

  </Project>